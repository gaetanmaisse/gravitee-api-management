{
  "id": "my-api",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "async",
  "description": "api v4 using SSE entrypoint",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test"
        }
      ],
      "entrypoints": [
        {
          "type": "http-get",
          "configuration": {
            "messageLimitCount": 2,
            "messageLimitDuration": 5000,
            "headersInPayload": true,
            "metadataInPayload": true
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default",
      "type": "kafka",
      "endpoints": [
        {
          "name": "default",
          "type": "kafka",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "topics": "test-topic",
            "bootstrapServers": "bootstrap-server",
            "consumer": {
              "autoOffsetReset": "earliest"
            }
          }
        }
      ]
    }
  ],
  "flows": []
}
